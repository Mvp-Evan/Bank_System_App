<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CreatAccount.CreatAccountController">
   <children>
      <GridPane layoutX="83.0" layoutY="113.0" prefHeight="191.0" prefWidth="434.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="ID" />
            <Label text="Name" GridPane.rowIndex="1" />
            <Label text="Address" GridPane.rowIndex="2" />
            <Label text="Passport" GridPane.rowIndex="3" />
            <Label prefHeight="15.0" prefWidth="159.0" text="Affirm Passport" GridPane.rowIndex="4" />
            <TextField fx:id="nameInput" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="addressInput" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <TextField fx:id="passportInput" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="passportAffirm" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="idRandom" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="okButton" layoutX="164.0" layoutY="335.0" mnemonicParsing="false" onMouseClicked="#collectData" text="OK" />
      <Button fx:id="cancelButton" layoutX="374.0" layoutY="335.0" mnemonicParsing="false" onMouseClicked="#backToMainScene" text="Back" />
      <Label fx:id="logonSuccess" layoutX="285.0" layoutY="59.0" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
